<h1>Products#new</h1>
<p>Find me in app/views/products/new.html.erb</p>

<%= simple_form_for @product do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :stock %>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Submit list", class: "btn btn-primary" %>
<% end %>
